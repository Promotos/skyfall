<%= link_to image_tag('/assets/icons/new.png', :title => t("common.new")), notes_new_path %>
<p>
<table id="sky_table">
	<tr>
		<th align="left">
			<%= t("note.id") %>
		</th>
		<th align="left">
			<%= t("note.title") %>
		</th>
		<th align="left">
			<%= t("note.created") %>
		</th>		
	</tr>
	<% @notes.each do |note| %>
	<tr>
		<td><%= link_to note.id, notes_show_path(:id=>note.id) %></td>		
		<td width="100%"> <%= note.note_id %> </td>
		<td width="100%"> <%= note.user.user_id %> </td>
		<td><%= link_to image_tag('/assets/icons/edit_table.gif'), notes_edit_path(:id=>note.id) %></td>		
		<td>
			<%= link_to image_tag('/assets/icons/remove_table.gif'), note, confirm: t("common.delete_confirm"), method: :delete %> 
		</td>		
	</tr>
	<% end %>
</table>
</p>